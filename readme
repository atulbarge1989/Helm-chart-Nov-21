apiVersion: v1
kind: PersistentVolume
metadata:
  name: pfs-dash-pv
  labels:
    app: pfs-dash
spec:
  capacity:
    storage: 500Gi
  volumeMode: Filesystem
  accessModes:
    - ReadWriteOnce
  persistentVolumeReclaimPolicy: Retain
  mountOptions:
    - hard
    - nfsvers=3
  nfs:
    path: /nfs
    server: test
---
apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: codesonar-hub0-pvc
  namespace: pfs-dash
spec:
  accessModes:
  - ReadWriteOnce
  resources:
    requests:
      storage: 500Gi
  selector:
    matchLabels:
      app: pfs-dash
